<Page x:Class="LibrarySystem.MemberBooksearch"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LibrarySystem"
      mc:Ignorable="d" 
      d:DesignHeight="417" d:DesignWidth="908"
      Title="MemberBooksearch">

    <Grid>
        <ListBox x:Name="listSearchResults" d:ItemsSource="{d:SampleData ItemCount=30}" Margin="379,-1,0,0"/>
        <Button x:Name="btnHome" Content="Button" HorizontalAlignment="Left" Margin="22,21,0,0" VerticalAlignment="Top" Width="54" Height="47" Click="btnHome_Click"/>

    </Grid>
</Page>
